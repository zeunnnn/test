{
    "mcpServers": {
        "national-weather-service": {
            "command": "cmd",
            "args": [
             "/c",
                "npx",
                "-y",
                "@smithery/cli@latest",
                "run",
                "@smithery-ai/national-weather-service",
                "--key",
                "58214f9b-ea5e-4f2a-80f2-ba87a40a9655"]
        },
        "weather": {
            "command": "python",
            "args": ["./mcp_server/mcp_server_weather.py"],
            "transport": "stdio"
        },
        "local_file_manager": {
            "command": "python",
            "args": ["./mcp_server/mcp_server_file_manager.py"],
            "transport": "stdio"
        }
    }
}